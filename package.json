{
    "name": "ballerini-management",
    "version": "1.0.0",
    "description": "Api de clientes e usuarios",
    "main": "server.ts",
    "scripts": {
        "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules ./src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "tsc && node dist/server.js",
        "pretty": "prettier --write \"src/**/*.ts\"",
        "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --coverage"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/CarlosZoft/balle-challenge-community"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CarlosZoft/balle-challenge-community/issues"
    },
    "homepage": "https://github.com/CarlosZoft/balle-challenge-community#readme",
    "husky": {
        "hooks": {
            "pre-commit": "npm run pretty && npm run lint"
        }
    },
    "engines": {
        "npm": ">=8.1.2",
        "node": ">=16.13.2"
    },
    "devDependencies": {
        "ts-jest": "^27.0.7",
        "ts-node": "^10.5.0",
        "ts-node-dev": "^1.1.8",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5",
        "uuid": "^8.3.2",
        "husky": "^7.0.4",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.3",
        "@types/node": "^17.0.21",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-no-loops": "^0.3.0",
        "prettier": "^2.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "supertest": "^6.1.6"
    },
    "dependencies": {
        "@prisma/client": "^3.10.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.5.1",
        "cookie-parser": "^1.4.6",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "jest": "^27.3.1",
        "onchange": "^7.1.0",
        "pg": "^8.7.3",
        "prisma": "^3.10.0"
    }
}
